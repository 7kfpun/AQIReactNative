{
  "name": "AQI",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "build-android": "npm run release-android; osascript -e 'display notification \"DONE!\" with title \"Android built\"'",
    "bundle-android": "$NVM_BIN/react-native bundle --platform android --dev false --entry-file ./index.android.js --bundle-output android/app/src/main/assets/index.android.bundle # --assets-dest android/app/src/main/res/",
    "release-android": "cd android && ./gradlew assembleRelease && cd ..",
    "install-android": "adb install android/app/build/outputs/apk/app-release.apk",
    "capture-android": "adb shell screencap -p | perl -pe 's/\\x0D\\x0A/\\x0A/g' > screenshot0.png",
    "test": "jest"
  },
  "dependencies": {
    "react": "15.4.2",
    "react-native": "0.41.2",
    "react-native-admob": "github:lucianomlima/react-native-admob#react-native_v0.40.0-fix",
    "react-native-android-location": "^1.0.2",
    "react-native-animatable": "^1.1.0",
    "react-native-device-info": "^0.10.1",
    "react-native-fbads": "LutherDiaz/react-native-fbads#patch-1",
    "react-native-google-analytics-bridge": "^5.0.1",
    "react-native-i18n": "^1.0.0",
    "react-native-location": "github:timfpark/react-native-location#723d46382d8c370f6e6b795006276f98e80b2d84",
    "react-native-maps": "github:airbnb/react-native-maps#1efb8efad50c2ed37a8c091cf7b28d8744eec0f3",
    "react-native-onesignal": "github:geektimecoil/react-native-onesignal",
    "react-native-root-toast": "^1.0.3",
    "react-native-router-flux": "^3.38.0",
    "react-native-simple-store": "^1.1.0",
    "react-native-star-rating": "^1.0.7",
    "react-native-store-review": "^0.1.1",
    "react-native-timer": "^1.1.3",
    "react-native-vector-icons": "^4.0.0",
    "shortid": "^2.2.8"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "babel-jest": "^19.0.0",
    "babel-preset-react-native": "1.9.1",
    "eslint": "^3.17.1",
    "eslint-config-airbnb": "14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.10.0",
    "jest": "19.0.2",
    "react-test-renderer": "15.4.2"
  },
  "jest": {
    "preset": "react-native"
  }
}
